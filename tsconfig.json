{
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "constants/*": ["src/constants/*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "models/*": ["src/models/*"],
      "pages/*": ["src/pages/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"],
    }
  },
  "include": ["**/*"]
}
